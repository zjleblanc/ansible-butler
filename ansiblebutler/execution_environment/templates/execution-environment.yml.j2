# Generated by ansible-butler
# {{ date.strftime('%m-%d-%y %H:%M') }}
---
version: {{ version }}

ansible_config: {{ ansible_config }}

build_arg_defaults:
  EE_BASE_IMAGE: {{ ee_base_image }}
  EE_BUILDER_IMAGE: {{ ee_builder_image }}

dependencies:
  galaxy: requirements.yml
  python: requirements.txt
  system: bindep.txt

{% if prepend_build_steps|length > 0 or append_build_steps|length > 0 %}
additional_build_steps:
  {% if prepend_build_steps|length > 0 %}
  prepend: |
    {% for prepend_step in prepend_build_steps %}
    {{ prepend_step }}
    {% endfor %}
  {% endif %}
  {% if append_build_steps|length > 0 %}
  append: |
    {% for append_step in append_build_steps %}
    {{ append_step }}
    {% endfor %}
  {% endif %}
{% endif %}